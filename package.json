{
  "name": "attio-tg",
  "version": "1.0.0",
  "description": "> Forward messages from Telegram and automatically add them to your Attio CRM with just a few taps.",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ai": "vitest run tests/ai",
    "test:ai:smoke": "vitest run tests/ai/intent.test.ts -t 'Smoke'",
    "test:attio": "vitest run tests/attio",
    "test:schema": "tsx scripts/test-schema.ts",
    "prepare": "husky"
  },
  "keywords": [
    "telegram",
    "bot",
    "attio",
    "crm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/gateway": "^2.0.18",
    "@vercel/analytics": "^1.6.1",
    "ai": "^5.0.106",
    "dotenv": "^17.2.3",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "vitest": "^4.0.15",
    "workflow": "^4.0.1-beta.26",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@vercel/node": "^5.5.14",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vercel": "^48.10.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,js,jsx}": [
      "prettier --write"
    ]
  }
}
